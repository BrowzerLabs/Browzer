{
  "id": "example-python-agent",
  "name": "Example Python Agent",
  "version": "1.0.0",
  "description": "An example Python-based AI agent extension",
  "author": "Browzer Team",
  "type": "python_agent",
  "main": "main.py",
  "permissions": [
    "python_execution",
    "ai_api_access",
    "network_access",
    "storage",
    "tabs"
  ],
  "files": [
    "main.py",
    "requirements.txt",
    "config.json"
  ],
  "icon": "icon.png",
  "configSchema": {
    "apiKey": {
      "type": "string",
      "description": "API key for AI service",
      "required": true
    },
    "model": {
      "type": "string",
      "description": "AI model to use",
      "default": "gpt-3.5-turbo",
      "options": ["gpt-3.5-turbo", "gpt-4", "claude-3-sonnet"]
    },
    "temperature": {
      "type": "number",
      "description": "Temperature for AI responses",
      "default": 0.7
    },
    "maxTokens": {
      "type": "number",
      "description": "Maximum tokens for responses",
      "default": 1000
    }
  },
  "minBrowserVersion": "1.0.0",
  "keywords": ["ai", "agent", "python", "automation"]
} 